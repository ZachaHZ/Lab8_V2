<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="bp" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.transversalTourController">
    <center>
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
            <children>
                <Pane fx:id="treePane" layoutY="87.0" prefHeight="402.0" prefWidth="600.0" style="-fx-background-color: #AEEAE0;">
                    <children>
                        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#randomizeOnAction" prefHeight="26.0" prefWidth="142.0" text="Randomize">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="166.0" layoutY="14.0" mnemonicParsing="false" onAction="#preOrderOnAction" prefHeight="26.0" prefWidth="142.0" text="Pre Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="320.0" layoutY="14.0" mnemonicParsing="false" onAction="#inOrderOnAction" prefHeight="26.0" prefWidth="142.0" text="In Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="474.0" layoutY="14.0" mnemonicParsing="false" onAction="#postOrderOnAction" prefHeight="26.0" prefWidth="142.0" text="Post Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
