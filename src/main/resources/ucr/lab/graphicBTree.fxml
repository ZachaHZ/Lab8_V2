<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane fx:id="bp" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GraphicBTreeController">
    <center>
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
            <children>
                <Pane prefHeight="87.0" prefWidth="600.0" style="-fx-background-color: #13A990;">
                    <children>
                        <Text layoutX="203.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Graphic Binary Tree">
                            <font>
                                <Font name="Bernard MT Condensed" size="25.0" />
                            </font>
                        </Text>
                    </children>
                </Pane>
                <Pane layoutY="87.0" prefHeight="402.0" prefWidth="600.0" style="-fx-background-color: #AEEAE0;">
                    <children>
                        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#randomizeOnAction" prefHeight="26.0" prefWidth="142.0" text="Randomize">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="166.0" layoutY="14.0" mnemonicParsing="false" onAction="#levesOnAction" prefHeight="26.0" prefWidth="142.0" text="Levels">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="320.0" layoutY="14.0" mnemonicParsing="false" onAction="#tourOnAction" prefHeight="26.0" prefWidth="142.0" text="Tour Info">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
