<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane fx:id="bp" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.transversalTourController">
    <center>
        <AnchorPane prefHeight="400.0" prefWidth="600.0">
            <children>
                <Pane prefHeight="73.0" prefWidth="600.0" style="-fx-background-color: #13A990;">
                    <children>
                        <Text layoutX="112.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Graphic Binary Tree - Transversal Tour">
                            <font>
                                <Font name="Bernard MT Condensed" size="25.0" />
                            </font>
                        </Text>
                    </children>
                </Pane>
                <Pane layoutY="73.0" prefHeight="430.0" prefWidth="600.0" style="-fx-background-color: #AEEAE0;">
                    <children>
                        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#randomizeOnAction" prefHeight="38.0" prefWidth="134.0" text="Randomize">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="159.0" layoutY="14.0" mnemonicParsing="false" onAction="#preOrderOnAction" prefHeight="38.0" prefWidth="134.0" text="Pre Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="300.0" layoutY="14.0" mnemonicParsing="false" onAction="#inOrderOnAction" prefHeight="38.0" prefWidth="134.0" text="In Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                        <Button layoutX="443.0" layoutY="14.0" mnemonicParsing="false" onAction="#postOrderOnAction" prefHeight="38.0" prefWidth="134.0" text="Post Order">
                            <font>
                                <Font name="Corbel Bold" size="19.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
